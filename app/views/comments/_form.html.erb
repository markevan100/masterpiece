
  <%= form_for @comment do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :score %>
    <%= f.text_field :score %>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <%= f.label :write %>
    <%= f.text_area :write %>
    <%= f.label :add %>
    <%= f.text_area :add %>

    <%= f.submit %>

  <% end %>
