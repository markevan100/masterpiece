
  <%= form_for @comment do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :participation %>
    <%= f.text_area :participation %>
    <%= f.label :score %>
    <%= f.text_field :score %>
    <%= f.label :test_c %>
    <%= f.text_area :test_c %>
    <%= f.label :write %>
    <%= f.text_area :write %>
    <%= f.label :rec %>
    <%= f.text_area :rec %>

    <%= f.submit %>

  <% end %>
